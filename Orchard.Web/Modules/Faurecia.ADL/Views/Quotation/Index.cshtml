                            </td>
                            <td style="padding:1px;">
                                @Html.HiddenFor(m => m.HeadCounts[index].ActivityType.ActivityType)
                                @(Model.HeadCounts[index].ActivityType.ActivityType)
                            </td>
                            <td style="padding:1px;">
                                <input type="text" style="padding:0px;width:50px;height:auto;" value="@Model.HeadCounts[index].Jan" name="@Html.NameOf(m => m.HeadCounts[index].Jan)" />
                            </td>
                            <td style="padding:1px;">
                                <input type="text" style="padding:0px;width:50px;height:auto;" value="@Model.HeadCounts[index].Feb" name="@Html.NameOf(m => m.HeadCounts[index].Feb)" />
