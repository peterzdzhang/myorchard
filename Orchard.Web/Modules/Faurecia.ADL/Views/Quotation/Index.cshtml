        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
        @for (int i = 0; i < Model.Years.Count; i++)
        {
            int year = Model.Years[i];
            if (year == Model.CurrentYear)
            {
                <li role="presentation" class="active"><a href="#@year" role="tab" data-toggle="tab">@year</a></li>
            }
            else
            {
                <li role="presentation"><a href="#@year" role="tab" data-toggle="tab">@year</a></li>
            }
        }
        </ul>
        <!-- Tab panes -->
        <div class="tab-content">
        @for (int i = 0; i < Model.Years.Count; i++)
        {
            int year = Model.Years[i];
            string cssClassName = year == Model.CurrentYear ? "active" : string.Empty;
            <div role="tabpanel" class="tab-pane @cssClassName" id="#@year">
            <!--HeadCounts-->
            @{ index = 0; }
            <div class="alert-info text-center" style="padding:5px">
                @T("HEAD COUNT")
            </div>
            <div style="overflow-y:inherit;font-size:12px">
                <table class="table table-bordered table-condensed table-hover">
                    <thead>
                        <tr class="active">
                            <th>@T("Comment")</th>
                            <th>@T("RMB/Hour")</th>
                            <th>@T("Cost Center")</th>
                            <th>@T("Type")</th>
                            <th>@T("Jan")</th>
                            <th>@T("Feb")</th>
                            <th>@T("Mar")</th>
                            <th>@T("Apr")</th>
                            <th>@T("May")</th>
                            <th>@T("Jun")</th>
                            <th>@T("Jul")</th>
                            <th>@T("Aug")</th>
                            <th>@T("Sep")</th>
                            <th>@T("Oct")</th>
                            <th>@T("Nov")</th>
