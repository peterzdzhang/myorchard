@model Faurecia.ADL.ViewModels.ADLViewModel
@using Faurecia.ADL.ViewModels;

@for (int i= 0;i < Model.Years.Count;i++)
{
    int index = 0;
    int currentYear = Model.Years[i];
    ADLDetailEntry entry = Model.Detail.Entries.SingleOrDefault(w => w.Year == currentYear);
    if (entry == null) { continue; }
    int entryIndex = Model.Detail.Entries.IndexOf(entry);
    string cssClassName = i==0? "tab-pane active" : "tab-pane";
    var tabName = string.Format("YEAR{1}_{0}", currentYear, Model.Head.Id);
    Dictionary<string, string> dicKickOff = new Dictionary<string, string>();

<div role="tabpanel" class="@cssClassName" id="@tabName" data-year="@currentYear" data-adlid="@Model.Head.Id">
    <!--HeadCounts-->
    @{ index = 0; }
    <div class="alert-info text-center" style="padding:5px;margin-top:2px">
        <div class="row">
            <div class="col-sm-11 col-xs-11 col-lg-11">
                @T("HEAD COUNT")
            </div>
            <div class="col-lg-1 col-sm-1 col-xs-1 text-right">
            </div>
        </div>
    </div>
    <div style="font-size:12px" class="form-horizontal collapse in" id="headCount_@currentYear">
        <table class="table table-bordered table-condensed" id="detailHeadCount_@entryIndex"  name="HeadCount" data-entryIndex="@entryIndex">
            <thead>
                <tr class="active">
                    <th nowrap>@T("Comment")</th>
                    <th nowrap>@T("RMB/Hour")</th>
                    <th nowrap>@T("Cost Center")</th>
                    <th nowrap>@T("Type")</th>
                    <th nowrap>@T("Jan")</th>
                    <th nowrap>@T("Feb")</th>
                    <th nowrap>@T("Mar")</th>
                    <th nowrap>@T("Apr")</th>
                    <th nowrap>@T("May")</th>
                    <th nowrap>@T("Jun")</th>
                    <th nowrap>@T("Jul")</th>
                    <th nowrap>@T("Aug")</th>
                    <th nowrap>@T("Sep")</th>
                    <th nowrap>@T("Oct")</th>
                    <th nowrap>@T("Nov")</th>
                    <th nowrap>@T("Dev")</th>
                    <th nowrap>@T("Y1")</th>
                </tr>
            </thead>
            <tr class="success">
                <td style="">&nbsp;</td>
                <td style=""></td>
                <td style=""></td>
                <td style=""></td>
               @for (int k = 1; k <= 12; k++)
                {
                    dicKickOff.Clear();
                    bool isStartDate = (Model.Head.StartDate != null && Model.Head.StartDate.Value.Year == currentYear && Model.Head.StartDate.Value.Month == k);
                    if (isStartDate)
                    {
                        dicKickOff.Add("Start", T("Start-{0}", Model.Head.StartDate.Value.Day).Text);
                    }
                    bool isPTRDate = (Model.Head.PTRDate != null && Model.Head.PTRDate.Value.Year == currentYear && Model.Head.PTRDate.Value.Month == k);
                    if (isPTRDate)
                    {
                        dicKickOff.Add("PTR", T("PTR-{0}", Model.Head.PTRDate.Value.Day).Text);
                    }
                    bool isMockup = (Model.Head.Mockup != null && Model.Head.Mockup.Value.Year == currentYear && Model.Head.Mockup.Value.Month == k);
                    if (isMockup)
                    {
                        dicKickOff.Add("Mockup", T("Mockup-{0}", Model.Head.Mockup.Value.Day).Text);
                    }
                    bool isOfferDate = (Model.Head.OfferDate != null && Model.Head.OfferDate.Value.Year == currentYear && Model.Head.OfferDate.Value.Month == k);
                    if (isOfferDate)
                    {
                        dicKickOff.Add("Offer", T("Offer-{0}", Model.Head.OfferDate.Value.Day).Text);
                    }
                    bool isAward = (Model.Head.Award != null && Model.Head.Award.Value.Year == currentYear && Model.Head.Award.Value.Month == k);
                    if (isAward)
                    {
                        dicKickOff.Add("Award", T("Award-{0}", Model.Head.Award.Value.Day).Text);
                    }
                    bool isProgramKickOff = (Model.Head.ProgramKickOff != null && Model.Head.ProgramKickOff.Value.Year == currentYear && Model.Head.ProgramKickOff.Value.Month == k);
                    if (isProgramKickOff)
                    {
                        dicKickOff.Add("Program", T("ProgramKickoff-{0}", Model.Head.ProgramKickOff.Value.Day).Text);
                    }
                    bool isProtoDate = (Model.Head.ProtoDate != null && Model.Head.ProtoDate.Value.Year == currentYear && Model.Head.ProtoDate.Value.Month == k);
                    if (isProtoDate)
                    {
                        dicKickOff.Add("Proto", T("Proto-{0}", Model.Head.ProtoDate.Value.Day).Text);
                    }
                    bool isDV = (Model.Head.DV != null && Model.Head.DV.Value.Year == currentYear && Model.Head.DV.Value.Month == k);
                    if (isDV)
                    {
                        dicKickOff.Add("DV", T("DV-{0}", Model.Head.DV.Value.Day).Text);
                    }
                    bool isToolingKickOff = (Model.Head.ToolingKickOff != null && Model.Head.ToolingKickOff.Value.Year == currentYear && Model.Head.ToolingKickOff.Value.Month == k);
                    if (isToolingKickOff)
                    {
                        dicKickOff.Add("Tooling", T("ToolingKickoff-{0}", Model.Head.ToolingKickOff.Value.Day).Text);
                    }
                    bool isPV = (Model.Head.PV != null && Model.Head.PV.Value.Year == currentYear && Model.Head.PV.Value.Month == k);
                    if (isPV)
                    {
                        dicKickOff.Add("PV", T("PV-{0}", Model.Head.PV.Value.Day).Text);
                    }
                    bool isSOPDate = (Model.Head.SOPDate != null && Model.Head.SOPDate.Value.Year == currentYear && Model.Head.SOPDate.Value.Month == k);
                    if (isSOPDate)
                    {
                        dicKickOff.Add("SOP", T("SOP-{0}", Model.Head.SOPDate.Value.Day).Text);
                    }
                    var kickOffCount = 0;
                <td style="">
                    @foreach (var kickOffKey in dicKickOff.Keys)
                    {
                        kickOffCount++;
                        if (kickOffCount > 3) { break; }
                        <button type="button" class="btn btn-sm btn-success">
                            @dicKickOff[kickOffKey]
                        </button>
                    }
                </td>
               }
